// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id         String   @id @default(uuid())
  nome       String
  email      String   @unique
  senhaHash  String
  senhaSalt  String
  created_at DateTime @default(now())

  @@map("usuarios")
}
